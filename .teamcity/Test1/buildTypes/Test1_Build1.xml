          <param name="perCheckinTriggering" value="true" />
          <param name="quietPeriod" value="345" />
          <param name="quietPeriodMode" value="USE_CUSTOM" />
          <param name="triggerRules"><![CDATA[+:**
-:**
+:user=foo;root=Test1_GithubComBradJonesTeamcityPlayground;comment=.*:**]]></param>
        </parameters>
      </build-trigger>
      <build-trigger id="TRIGGER_1" type="schedulingTrigger">
